const HN2_Get_Fas =function( src_pat ){

    const RESOLVER=(a)=>{ return( a ); };
    const REJECTOR=(a)=>{ return( a ); };
    const EXECUTOR=( resolver , rejector )=>{

        fs.readFile( src_pat,function(obj_err,dat_fil ){
            if( obj_err ){

                REJECTOR( obj_err );

            }else{

                RESOLVER( dat_fil );

            };;
        });;
    };;

    var pro=( new Promise( EXECUTOR ) );
